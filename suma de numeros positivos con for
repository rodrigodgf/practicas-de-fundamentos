#include <stdio.h>



   

int main() 

{
    
    float SUM = 0 , NUM = 0;
    int i;
    int rep = 0;
    
    
    printf("cu√°ntas veces se va a repetir?");
    scanf("%d", &rep);
    
    for (i=1; i <= rep; i++) {
      printf("ingresa tus numeros: ");
        scanf("%f", &NUM);
         
        
        if (NUM>=1) {
            SUM+=NUM;    
            
        }
        
        
       
    }
    printf("la suma de tus numero es: %.1f" , SUM);
    
    return 0;
}
